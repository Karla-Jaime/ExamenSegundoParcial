<UserControl x:Class="ExamenSegundoParcial.Contacto"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ExamenSegundoParcial"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel >
        <TextBlock FontFamily="arial" FontSize="16" Margin="20,5,20,5" >Correo:</TextBlock>
        <TextBox Name="BoxCorreo" Margin="20,5,20,5"></TextBox>
        <TextBlock FontFamily="arial" FontSize="16" Margin="20,5,20,5" >Teléfono:</TextBlock>
        <TextBox Name="BoxTele"  Margin="20,5,20,5"></TextBox>
        <TextBlock FontFamily="arial" FontSize="16" Margin="20,5,20,5" >Celular:</TextBlock>
        <TextBox Name="BoxCelular"  Margin="20,5,20,5"></TextBox>
    </StackPanel>
</UserControl>
