<UserControl x:Class="ExamenSegundoParcial.Informacion_Personal"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ExamenSegundoParcial"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Background="Beige">
        <TextBlock FontFamily="arial" FontSize="16" Margin="20,5,20,5" >Nombre:</TextBlock>
        <TextBox Name="BoxN" Margin="20,5,20,5"></TextBox>
        <TextBlock FontFamily="arial" FontSize="16" Margin="20,5,20,5" >Apellido Paterno:</TextBlock>
        <TextBox Name="BoxApP"  Margin="20,5,20,5"></TextBox>
        <TextBlock FontFamily="arial" FontSize="16" Margin="20,5,20,5" >Apellido Materno:</TextBlock>
        <TextBox Name="BoxApM"  Margin="20,5,20,5"></TextBox>
        <TextBlock FontFamily="arial" FontSize="16" Margin="20,5,20,5" >Edad:</TextBlock>
        <TextBox Name="BoxEdad" Margin="20,5,20,5"></TextBox>
    </StackPanel>
</UserControl>
